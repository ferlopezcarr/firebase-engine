{
  "name": "firebase-engine",
  "version": "1.2.6",
  "description": "Tools  for firebase (backup, clean,  restore)",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "build": "rm -rf lib && tsc --declaration --declarationMap --sourceMap",
    "doc": "typedoc src&&echo \"\">./docs/.nojekyll",
    "test": "firebase emulators:exec \"mocha --exit test/**/*.test.js\"",
    "cov": "firebase emulators:exec \"nyc mocha --exit test/**/*.test.js\""
  },
  "bin": "./lib/bin/firebase-engine.js",
  "repository": {
    "type": "git",
    "url": "github:siarheidudko/firebase-engine"
  },
  "homepage": "https://siarheidudko.github.io/firebase-engine/",
  "engines": {
    "node": ">=10.12"
  },
  "keywords": [
    "firebase",
    "firestore",
    "auth",
    "storage",
    "backup",
    "clean",
    "restore",
    "copy",
    "move",
    "cli",
    "mobile"
  ],
  "author": "Siarhei Dudko <slavianich@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@google-cloud/storage": "^5.0.1",
    "@sergdudko/objectstream": "^2.0.4",
    "firebase-admin": "^8.12.1"
  },
  "devDependencies": {
    "@types/node": "^14.0.5",
    "mocha": "^7.2.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.3"
  }
}
