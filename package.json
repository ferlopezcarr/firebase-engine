{
  "name": "firebase-engine",
  "version": "1.2.7",
  "description": "Tools  for firebase (backup, clean,  restore)",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "build": "rm -rf lib && tsc --declaration",
    "doc": "typedoc src&&echo \"\">./docs/.nojekyll&&echo \"firebase-engine.ml\">./docs/CNAME",
    "test": "firebase emulators:exec \"mocha --exit test/**/*.test.js\"",
    "cov": "firebase emulators:exec \"nyc mocha --exit test/**/*.test.js\""
  },
  "bin": "./lib/bin/firebase-engine.js",
  "repository": {
    "type": "git",
    "url": "github:siarheidudko/firebase-engine"
  },
  "homepage": "https://firebase-engine.ml",
  "engines": {
    "node": ">=10.12"
  },
  "keywords": [
    "firebase",
    "firestore",
    "auth",
    "storage",
    "backup",
    "clean",
    "restore",
    "copy",
    "move",
    "cli",
    "mobile"
  ],
  "author": "Siarhei Dudko <slavianich@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@google-cloud/storage": "^5.0.1",
    "@sergdudko/objectstream": "^2.0.5",
    "firebase-admin": "^8.12.1"
  },
  "devDependencies": {
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "mocha": "^7.2.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5"
  }
}
